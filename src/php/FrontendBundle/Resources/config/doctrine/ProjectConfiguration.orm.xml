<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Frontastic\Catwalk\FrontendBundle\Domain\ProjectConfiguration" table="project_configuration">
        <id name="projectConfigurationId" column="pc_id" type="string">
            <generator strategy="NONE" />
        </id>

        <field name="configuration" column="pc_configuration" type="object" />

        <field name="metaData" column="pc_meta_data" type="object" />
        <field name="sequence" column="pc_sequence" type="string" />
        <field name="isDeleted" column="pc_is_deleted" type="boolean" />
    </entity>
</doctrine-mapping>
