<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <!-- parameters>
    </parameters -->

    <services>
        <defaults public="true" />

        <service id="Frontastic\Catwalk\KameleoonBundle\Symfony\TerminateListener">
            <argument type="service" id="Frontastic\Catwalk\KameleoonBundle\Domain\TrackingService"/>

             <tag name="kernel.event_listener" event="kernel.terminate" />
        </service>

        <service id="Frontastic\Catwalk\KameleoonBundle\Domain\TrackingService">
        </service>
    </services>
</container>
